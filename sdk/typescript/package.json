{
  "name": "@openchat/typescript-sdk",
  "version": "1.0.0",
  "description": "OpenChat TypeScript SDK - 高内聚低耦合的即时通讯SDK，基于悟空IM EasySDK，支持多平台、多RTC Provider",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "keywords": [
    "openchat",
    "im",
    "chat",
    "instant-messaging",
    "typescript",
    "sdk",
    "wukongim",
    "volcengine",
    "rtc",
    "webrtc",
    "miniprogram",
    "wechat",
    "alipay",
    "baidu",
    "bytedance",
    "ai",
    "media-resource"
  ],
  "author": "OpenChat Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/openchat-team/openchat-typescript-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/openchat-team/openchat-typescript-sdk/issues"
  },
  "homepage": "https://github.com/openchat-team/openchat-typescript-sdk#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.6.0",
    "rollup-plugin-dts": "^6.1.0",
    "ts-jest": "^29.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.0"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.esm.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.esm.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./core": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.esm.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./services": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.esm.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./rtc": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.esm.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "typesVersions": {
    "*": {
      "types": [
        "dist/index.d.ts"
      ],
      "core": [
        "dist/index.d.ts"
      ],
      "services": [
        "dist/index.d.ts"
      ],
      "rtc": [
        "dist/index.d.ts"
      ]
    }
  }
}
